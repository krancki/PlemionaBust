{
  "manifest_version": 2,
  "name": "Plemiona Bust",
  "description": "I am to lazy to click",
  "version": "1.0.0",
  "icons": {
    "16": "favicon.png",
    "32": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "favicon.png",
      "32": "favicon.png",
      "48": "favicon.png",
      "128": "favicon.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.plemiona.pl/*"],
      "run_at": "document_end",
      "js": ["bustEngine.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": ["activeTab","storage"]

}